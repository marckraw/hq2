{
  "name": "grid.ef.design",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "debug-env": "node -e \"console.log('ENV VARS:', Object.keys(process.env).filter(k => k.includes('DATABASE') || k.includes('API') || k.includes('RAILWAY')).map(k => k + '=' + (process.env[k] ? 'SET' : 'UNSET')).join('\\n'))\"",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check": "npm run type-check && npm run lint && npm run test:run",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "eval": "npx tsx evals/run.ts $1",
    "eval:evalite:dev": "evalite watch",
    "eval:evalite:ci": "evalite --threshold=50",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:memory": "vitest run packages/thegrid/src/tests/memory.test.ts",
    "test:qdrant": "npx tsx src/scripts/test-qdrant-memory.ts",
    "test:conversational": "npx tsx src/scripts/test-conversational-memory.ts",
    "irf:example": "npx tsx src/services/AgentsFactories/FigmaToStoryblokAgent/IRF/example.ts",
    "test:agentic-core": "npx tsx src/examples/test-agentic-core.ts",
    "postman:generate": "tsx scripts/generate-postman.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.4",
    "@ai-sdk/openai": "^2.0.14",
