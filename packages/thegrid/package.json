    "@types/node": "^20.11.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoevals": "^0.0.129",
    "chalk": "^4.1.2",
    "drizzle-kit": "^0.28.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
