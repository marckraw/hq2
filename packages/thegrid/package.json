    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/genai": "^0.7.0",
    "@hono/node-server": "^1.13.2",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^0.16.4",
    "@hono/zod-validator": "^0.4.1",
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@mrck-labs/anton-sdk": "^1.3.0",
    "@mrck-labs/grid-agents": "^0.7.6",
    "@mrck-labs/grid-core": "^0.30.0",
    "@mrck-labs/grid-workflows": "^10.0.0",
    "@octokit/rest": "^21.0.2",
    "@types/pg": "^8.11.10",
    "ai": "^5.0.14",
    "axios": "^1.8.4",
    "core.mrck.dev": "workspace:*",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.1",
    "drizzle-zod": "^0.8.2",
    "fluent-ffmpeg": "^2.1.3",
    "hono": "^4.6.4",
    "hono-pino": "^0.3.0",
    "jsondiffpatch": "^0.7.3",
    "langfuse": "^3.30.3",
    "lowdb": "^7.0.1",
    "openai": "^5.9.0",
